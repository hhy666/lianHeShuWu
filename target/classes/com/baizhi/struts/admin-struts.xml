<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN" "struts-2.1.7.dtd" >
<struts>
    <package name="admin" extends="struts-default" namespace="/admin">
        <action name ="*" class="com.baizhi.action.AdminAction" method="{1}">
            <result name="queryAll">/index.jsp</result>
            <result name="loginOk">/back/main/main.jsp</result>
            <result name="loginError">/back/login.jsp</result>
            <result name="exit" type="redirect">/back/login.jsp</result>
        </action>
    </package>
	
</struts>
